  "dependencies": {
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "bitcoinjs-lib": "6.0.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-fetch": "3.1.5",
    "did-jwt": "6.6.0",
    "did-jwt-vc": "3.1.0",
    "did-resolver": "4.0.0",
    "express": "4.18.1",
    "mongodb": "4.9.0",
    "mongoose": "6.5.3",
    "pubsub-js": "1.9.4",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.5.0",
    "uuid": "8.3.2",
    "winston": "3.8.1",
    "ws": "8.8.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.7.13",
    "@types/pubsub-js": "1.8.3",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "copyfiles": "2.4.1",
    "env-cmd": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.8.2"
  },
  "scripts": {
    "build": "npm-run-all api:build ui:build ui:copy",
    "run": "node ./build",
    "start": "npm-run-all --parallel dev:api dev:ui",
    "dev:api": "env-cmd ts-node-dev --respawn api",
    "dev:ui": "npm run --prefix ui start",
    "dev:ui2": "cd ui && npm run start",
    "setup": "npm ci && cd ui && npm ci",
    "api:build": "tsc",
    "ui:build": "npm run --prefix ui build",
    "ui:copy": "copyfiles -V -a -u 2 \"ui/dist/**/*\" build/ui/",
    "version": "node -p \"require('./package.json').version\""
  }
}
