  "dependencies": {
    "bent": "7.3.12",
    "bitcoinjs-lib": "5.2.0",
    "bs58": "4.0.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-fetch": "3.1.4",
    "did-jwt": "5.6.3",
    "did-jwt-vc": "2.1.6",
    "did-resolver": "3.1.0",
    "express": "4.17.1",
    "mongodb": "3.6.9",
    "mongoose": "5.13.8",
    "pubsub-js": "1.9.3",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@types/bs58": "4.0.1",
    "@types/express": "4.17.13",
    "@types/node": "18.0.4",
    "@types/pubsub-js": "1.8.2",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "copyfiles": "2.4.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  },
  "scripts": {
    "build": "npm-run-all api:build ui:build ui:copy",
    "run": "node ./build",
    "start": "npm-run-all --parallel dev-api dev-ui",
    "dev-api": "ts-node-dev --respawn api",
    "dev-ui": "cd ui && npm run start",
    "setup": "npm ci && cd ui && npm ci",
    "api:build": "tsc",
    "ui:build": "cd ui && npm run prod",
    "ui:copy": "copyfiles -V -a -u 2 \"ui/dist/**/*\" build/ui/",
    "version": "node -p \"require('./package.json').version\""
  }
}
